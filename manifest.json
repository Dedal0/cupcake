{
  "name": "Cupcake",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Make a bridge automagically!",
  "background": { "page": "proxy.html",
    "persistent": true},
  "icons": {
    "24" : "images/24.png",
    "38" : "images/48.png",
    "48" : "images/48.png",
    "128" : "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/24.png",
    "default_title": "Cupcake",
    "default_popup": "menu.html"
  },
   "incognito": "spanning",
  "permissions": [
    "https://crypto.stanford.edu/*",
    "background",
    "cookies" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://crypto.stanford.edu/flashproxy/; object-src 'self' https://crypto.stanford.edu/flashproxy/"
  }