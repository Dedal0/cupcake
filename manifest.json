{
  "name": "accomodaTor",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Make a bridge automagically!",
  "background_page" : "background.html",
  "background": {"persistent": true},
  "icons": {
    "16" : "images/19.png",
    "38" : "images/38.png",
    "48" : "images/48.png",
    "128" : "images/128.png"
  },
  "browser_action": {
    "default_icon": "19.png",
    "default_title": "accomodaTor",
    "default_popup": "menu.html"
  },
   "incognito": "split",
  "permissions": [
    "background",
    "cookies",
],
}